<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.AntonSibgatulin.services">

    <class name="Anticheat" table="anticheat">
        <meta attribute="class-description">
            This class contains the Anticheat detail.
        </meta>
        <id name="id" type="int" column="id">

        </id>
        <property name="timeLastCreateMap" column="timeLastCreateMap" type="long"/>
